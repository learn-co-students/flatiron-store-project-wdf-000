<nav class="">
    <ul>
        <li> <%= link_to "Store Home", store_path %> </li>
        <% if user_signed_in? && current_user_cart %>
          <li> <%= link_to "Cart", cart_path(current_user_cart) %></li>
        <% end %>
        <% if user_signed_in? %>
          Signed in as <%= current_user.email %>
